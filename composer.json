{
    "type": "project",
    "license": "proprietary",
    "description": "Site platform for PHP, based on Symfony framework.",
    "authors": [
        {
            "name": "Ihor Drevetskyi",
            "email": "ihor.drevetskyi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "a2lix/translation-form-bundle": "^3.0",
        "friendsofsymfony/ckeditor-bundle": "^2.0",
        "gedmo/doctrine-extensions": "^2.4",
        "google/apiclient": "^2.0",
        "helios-ag/fm-elfinder-bundle": "^9.1",
        "hwi/oauth-bundle": "^0.6.3",
        "ihor-drevetskyi/component": "*@dev",
        "ihor-drevetskyi/dashboard": "*@dev",
        "ihor-drevetskyi/news": "*@dev",
        "ihor-drevetskyi/seo": "*@dev",
        "ihor-drevetskyi/static": "*@dev",
        "ihor-drevetskyi/upload": "*@dev",
        "ihor-drevetskyi/user": "*@dev",
        "jms/i18n-routing-bundle": "^3.0",
        "knplabs/doctrine-behaviors": "^1.5",
        "knplabs/knp-paginator-bundle": "^2.8",
        "nochso/html-compress-twig": "^2.0",
        "php-http/client-common": "^1.3",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "^1.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug": "^1.0",
        "php-http/httplug-bundle": "^1.11",
        "php-http/message-factory": "^1.0",
        "psr/http-message": "^1.0",
        "sensio/framework-extra-bundle": "^5.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.2.*",
        "symfony/config": "*",
        "symfony/console": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/expression-language": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.2.*",
        "symfony/framework-bundle": "4.2.*",
        "symfony/http-kernel": "*",
        "symfony/intl": "*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "*",
        "symfony/process": "4.2.*",
        "symfony/requirements-checker": "^1.1",
        "symfony/security-bundle": "4.2.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/validator": "4.2.*",
        "symfony/var-dumper": "*",
        "symfony/web-link": "4.2.*",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "4.2.*",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.5"
    },
    "require-dev": {
        "sensiolabs/security-checker": "^5.0",
        "symfony/browser-kit": "*",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.8",
        "symfony/phpunit-bridge": "*",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "component-dir": "public/assets"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "BackendBundle\\": "src/BackendBundle",
            "FrontendBundle\\": "src/FrontendBundle",
            "IhorDrevetskyi\\NewsBundle\\": "bundles/NewsBundle/src",
            "IhorDrevetskyi\\SupportCenter\\ContactBundle\\": "bundles/SupportCenter/ContactBundle/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "requirements-checker": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "4.2.*"
        }
    }
}
